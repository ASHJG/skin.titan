<?xml version="1.0" encoding="utf-8"?>
<!--Titan-->
<window>
    <defaultcontrol always="true">300</defaultcontrol>
	<include>animation_window_open_close</include>
	<animation effect="zoom" start="100" end="135" center="auto" easing="out" tween="back" time="0" condition="Skin.HasSetting(EnableTouchSupport)">Conditional</animation>
	<animation effect="slide" start="0,0" end="0,-75" center="auto" easing="out" tween="back" time="0" condition="Skin.HasSetting(EnableTouchSupport)">Conditional</animation>
    <zorder>7</zorder>
    <controls>
        <!--Dialog Overlay-->
		<control type="group">
			<visible>!Window.IsActive(MyVideoNav.xml)</visible>
			<include condition="!skin.hastheme(classic)">DialogOverlay</include>
			<include condition="skin.hastheme(classic)">DialogOverlayExtra</include>
        </control>
        <!--Keybord-->
		<include condition="!SubString(System.BuildVersion,14,left)">DialogKeyboardGotham</include>
		<include condition="SubString(System.BuildVersion,14,left)">DialogKeyboardHelix</include>
    </controls>
</window>