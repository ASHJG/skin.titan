<?xml version="1.0" encoding="utf-8"?>
<!--Titan-->
<window id="113">
    <defaultcontrol always="true">901</defaultcontrol>
    <allowoverlay>yes</allowoverlay>
    <zorder>3</zorder>
    <controls>
        <control type="group">
		
            <animation type="WindowOpen">
                <effect type="slide" start="200,0" end="0,0" time="400" tween="cubic" easing="inout" delay="0" />
            </animation>
            <animation type="WindowClose">
                <effect type="slide" start="0,0" end="200,0" time="400" tween="cubic" easing="inout" delay="0" />
            </animation>
			
			
            <control type="image">
				<right>40</right>
                <bottom>60</bottom>
				<width>100</width>
				<height>100</height>
				<colordiffuse>EEFF0000</colordiffuse>
				<texture>dialogs/volume/volume-mute-back.png</texture>
				<aspectratio>stretch</aspectratio>
            </control>
            
            <control type="image">
                <right>40</right>
                <bottom>60</bottom>
				<width>100</width>
				<height>100</height>
				<colordiffuse>EEFF0000</colordiffuse>
			<texture>dialogs/volume/mask.png</texture>
				<aspectratio>stretch</aspectratio>
            </control>
			

	</control>
    </controls>
</window>
